import random

chars = "ABCDEFGHIJKLMNOPQRSTUWVZXabcdefghijklmnopqrstuvwzx1234567890"

numbers = input('How many passwords you want? ')

numbers = int(numbers)

length = input("How many character you want your password to be? ")

length = int(length)

for pwd in range(numbers):
    passwords = ''
    for c in range(length):
        passwords += random.choice(chars)
    print(passwords)
